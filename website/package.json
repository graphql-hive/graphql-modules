{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "next build --turbopack || next build --webpack",
    "dev": "next --turbopack || next --webpack",
    "pagefind": "pagefind --site .next/server/app --output-path out/_pagefind",
    "postbuild": "next-sitemap && yarn pagefind",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@theguild/components": "^9.1.2",
    "next": "16.1.6",
    "next-sitemap": "4.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "pagefind": "1.4.0",
    "@theguild/tailwind-config": "0.6.4",
    "@types/node": "24.10.9",
    "@types/react": "19.2.10",
    "cross-env": "10.1.0",
    "postcss-import": "16.1.1",
    "tailwindcss": "3.4.19",
    "postcss-lightningcss": "1.0.2",
    "typescript": "5.9.3"
  }
}
